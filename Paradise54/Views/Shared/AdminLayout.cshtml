
@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="tr">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <title>Eatio - Restaurant Food Order Bootstrap Admin Dashboard</title>
    <!-- Favicon icon -->
    <link rel="icon" type="image/png" sizes="16x16" href="~/adminTheme/images/favicon.png">

    <link href="~/adminTheme/vendor/bootstrap-select//dist/css/bootstrap-select.min.css" rel="stylesheet">
    <link href="~/adminTheme/css/style.css" rel="stylesheet">

</head>

<body>

    <!--*******************
        Preloader start
    ********************-->
    <div id="preloader">
        <div class="sk-three-bounce">
            <div class="sk-child sk-bounce1"></div>
            <div class="sk-child sk-bounce2"></div>
            <div class="sk-child sk-bounce3"></div>
        </div>
    </div>
    <!--*******************
        Preloader end
    ********************-->
    <!--**********************************
        Main wrapper start
    ***********************************-->
    <div id="main-wrapper">

        <!--**********************************
        Nav header start
    ***********************************-->
        <div class="nav-header">
            <a href="index.html" class="brand-logo">
                <img class="logo-abbr" src="~/adminTheme/images/logo.png" alt="">
                <img class="logo-compact" src="~/adminTheme/images/logo-text.png" alt="">
                <img class="brand-title" src="~/adminTheme/images/logo-text.png" alt="">
            </a>

            <div class="nav-control">
                <div class="hamburger">
                    <span class="line"></span><span class="line"></span><span class="line"></span>
                </div>
            </div>
        </div>
        <!--**********************************
        Nav header end
    ***********************************-->

        <!--**********************************
        Header start
    ***********************************-->
        @await Html.PartialAsync("AdminPartialHeader")
        <!--**********************************
        Header end ti-comment-alt
    ***********************************-->
        <!--**********************************
        Sidebar start
    ***********************************-->
        @await Html.PartialAsync("AdminPartialNav")
        <!--**********************************
        Sidebar end
    ***********************************-->
        <!--**********************************
        Content body start
    ***********************************-->
        @RenderBody()
        <!--**********************************
        Content body end
    ***********************************-->
    </div>
    <!--**********************************
        Main wrapper end
    ***********************************-->
    <!--**********************************
        Scripts
    ***********************************-->
    <!-- Required vendors -->
    <script src="~/adminTheme/vendor/global/global.min.js"></script>
    <script src="~/adminTheme/vendor/bootstrap-select/dist/js/bootstrap-select.min.js"></script>
    <script src="~/adminTheme/vendor/chart.js/Chart.bundle.min.js"></script>
    <script src="~/adminTheme/js/custom.min.js"></script>
    <script src="~/adminTheme/js/deznav-init.js"></script>
    <!-- Apex Chart -->
    <script src="~/adminTheme/vendor/apexchart/apexchart.js"></script>
    <script>
		$(function () {
			$('[data-toggle="popover"]').popover()
		})
    </script>

</body>
</html>